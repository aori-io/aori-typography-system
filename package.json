{
  "name": "@aori/fonts",
  "version": "1.0.4",
  "description": "Aori Font System",
  "main": "dist/css/index.css",
  "style": "dist/css/index.css",
  "types": "src/types/aori__fonts.d.ts",
  "files": ["dist/**/*", "src/**/*", "tailwind.preset.js"],
  "exports": {
    ".": "./dist/css/index.css",
    "./css": "./dist/css/index.css",
    "./fonts": "./dist/fonts/",
    "./fonts/static": "./dist/fonts/static/",
    "./tailwind": "./tailwind.preset.js",
    "./font/sans": {
      "types": "./src/types/aori__fonts.d.ts",
      "default": "./src/font/sans/sans.js"
    },
    "./font/mono": {
      "types": "./src/types/aori__fonts.d.ts",
      "default": "./src/font/mono/mono.js"
    },
    "./font": {
      "types": "./src/types/aori__fonts.d.ts",
      "default": "./src/font/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["font", "typography", "aori"],
  "author": "Aori",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aori-io/aori-typography-system.git"
  },
  "homepage": "https://github.com/aori-io/aori-typography-system#readme",
  "peerDependencies": {
    "next": ">=13.0.0"
  }
}
